modules = ["python-3.10"]

[deployment]
run = ["sh", "-c", "python main.py"]
deploymentTarget = "cloudrun"

[env]
PYTHONPATH = "${PYTHONPATH}:${PWD}/src"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", ".replitignore", "main.py", "requirements.txt"]

[nix]
channel = "stable-22_11"

[unitTest]
language = "python3"

[debugger]
support = true

[interpreter]
command = ["python3", "-u", "main.py"]
